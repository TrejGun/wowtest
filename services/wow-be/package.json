{
  "dependencies": {
    "@nestjs/common": "6.5.3",
    "@nestjs/core": "6.5.3",
    "@nestjs/platform-express": "6.5.3",
    "@nestjs/swagger": "3.1.0",
    "@nestjs/typeorm": "6.1.3",
    "@package/date": "0.0.1",
    "@package/types": "0.0.1",
    "dotenv": "8.0.0",
    "lodash": "4.17.14",
    "pg": "7.12.1",
    "swagger-ui-express": "4.0.7",
    "typeorm": "0.2.18"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.0",
    "@types/faker": "4.1.5",
    "@types/mocha": "5.2.7",
    "@types/node": "12.0.10",
    "faker": "4.1.0",
    "mocha": "6.1.4",
    "supertest": "4.0.2",
    "ts-node": "8.3.0",
    "tsc-watch": "2.2.1",
    "typescript": "3.5.2"
  },
  "license": "UNLICENSED",
  "main": "./dist/src/index.js",
  "name": "@service/wow-be-service",
  "private": true,
  "scripts": {
    "build": "tsc",
    "entity:create": "npm run typeorm -- entity:create -n",
    "lint": "tslint --project tsconfig.json --fix",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run": "npm run typeorm migration:run",
    "start": "NODE_ENV=development tsc-watch --onSuccess \"node .\" --onFailure \"echo Compilation Failed\"",
    "start:debug": "NODE_ENV=development tsc-watch --onSuccess \"node --inspect=0.0.0.0:$DEBUG_PORT .\" --onFailure \"echo Compilation Failed\"",
    "start:prod": "NODE_ENV=production node .",
    "test": "exit 0",
    "typeorm": "NODE_ENV=development ts-node ./node_modules/.bin/typeorm --config src/ormconfig.ts"
  },
  "title": "WOW BE Service",
  "version": "0.0.1"
}
